<Location />
RewriteEngine On

RewriteCond %{HTTP_USER_AGENT} (iPhone|Android.*Mobile)
RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^ {{ web_host_base }}/mobile/login [L,R]

RewriteCond %{HTTP_USER_AGENT} !(iPhone|Android.*Mobile)
RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^ {{ legacy_host_base }}%{REQUEST_URI} [L,R]
</Location>

